{
  "name": "template-nextjs-monorepo",
  "version": "0.0.0",
  "engines": {
    "node": "22.14.0",
    "npm": "pnpmを使用してください",
    "yarn": "pnpmを使用してください"
  },
  "scripts": {
    "check-code": "run-p -c lint",
    "fix": "run-p -c \"fix:*\"",
    "fix:fixpack": "fixpack",
    "fix:prettier": "prettier \"./*.{json,toml,yaml,yml}\" \"!pnpm-lock.yaml\" \"!package.json\" -w -u",
    "lint": "run-p -c \"lint:*\"",
    "lint:fixpack": "fixpack --dryRun",
    "lint:prettier": "prettier \"./*.{json,toml,yaml,yml}\" \"!pnpm-lock.yaml\" \"!package.json\" -c -u",
    "prepare": "run-p \"prepare:*\"",
    "prepare:husky": "husky",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "fixpack": "4.0.0",
    "husky": "9.1.7",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3"
  },
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.6.3"
}
