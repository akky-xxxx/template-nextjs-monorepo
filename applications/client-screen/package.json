{
  "name": "client-screen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "check-code": "run-p -c lint spell-check",
    "fix": "run-p -c \"fix:*\"",
    "fix:eslint-other-than-src": "eslint -c ./eslint.config.other-than-src.mjs \"./\" --max-warnings 0 --fix",
    "fix:eslint-src": "eslint \"./src/\" --max-warnings 0 --fix",
    "fix:fixpack": "fixpack",
    "fix:prettier": "prettier \"./*.{json,toml,yaml,yml}\" \"!pnpm-lock.yaml\" \"!package.json\" -w -u",
    "lint": "run-p -c \"lint:*\"",
    "lint:eslint-other-than-src": "eslint -c ./eslint.config.other-than-src.mjs \"./\" --max-warnings 0",
    "lint:eslint-src": "eslint \"./src/\" --max-warnings 0",
    "lint:fixpack": "fixpack --dryRun",
    "lint:prettier": "prettier \"./*.{json,toml,yaml,yml}\" \"!pnpm-lock.yaml\" \"!package.json\" -c -u",
    "spell-check": "cspell lint --no-progress \"./**/*.{js,mjs,cjs}\" \"./*.{js,mjs,cjs}\"",
    "type-check": "tsc"
  },
  "dependencies": {
    "next": "15.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@tsconfig/next": "2.0.3",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "cspell": "8.17.5",
    "eslint": "9.22.0",
    "eslint-config-sc-all": "0.1.1",
    "eslint-config-sc-jest": "0.1.1",
    "eslint-config-sc-next": "0.1.1",
    "eslint-config-sc-storybook": "0.1.1",
    "eslint-config-sc-ts": "0.1.1",
    "fixpack": "4.0.0",
    "jest": "29.7.0",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "template-nextjs-monorepo": "workspace:*",
    "typescript": "5.8.2"
  }
}
